<!-- == Environment ======================================================== -->
<environment>
<propagation entity="propagation" />
<interferences entity="interferences"/>
<modulation entity="bpsk"/>
<!-- <monitor entity="monitor"/> -->
<!-- <noise entity="noise"/> -->
</environment>

<!-- == Bundle == (sets up how modules interact at each node) ============== -->
<bundle name="sensor" worldsens="false" default="true" birth="0">
  <mobility entity="mobility"/>
  <energy entity="battery"/>
  <antenna entity="antenna">
    <up entity="radio"/>
  </antenna>
  <with entity="radio">
    <up entity="mac"/>
    <down entity="antenna"/>
  </with>
  <with entity="mac">
    <up entity="routing"/>
    <down entity="radio"/>
  </with>
  <with entity="routing">
    <up entity="application"/>
    <down entity="mac"/>
  </with>
  <with entity="application">
    <down entity="routing"/>
  </with>

</bundle>

